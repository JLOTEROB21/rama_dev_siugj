@charset "UTF-8";
* {
	box-sizing:border-box;
	margin-top: 0px;
}

.magnify 
{
	z-index: 20;
	position: relative;
	margin:0px auto;
	overflow: hidden;
}

.magnify img {
	display:block;
}

.element_to_magnify {
	position: relative;
  	overflow: hidden;
}

.magnified_element 
{
	
}

.magnify_glass 
{
	z-index: 300;
	position: absolute;
	background-color:#FFF;
	border: 4px solid #CCC;
	border-radius: 50%;
	width: 130px;
	height: 130px;
	overflow:hidden;
}

