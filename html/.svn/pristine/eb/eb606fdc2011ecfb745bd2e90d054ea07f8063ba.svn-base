/*========================================================
                    HEADER styles
=========================================================*/
.header {
  background: #f0f0f0;
}
.header.page2 {
  padding-bottom: 26px;
}
.header-wrapper {
  position: relative;
}
.absolute {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
}
.company {
  float: left;
  margin-top: 21px;
}
