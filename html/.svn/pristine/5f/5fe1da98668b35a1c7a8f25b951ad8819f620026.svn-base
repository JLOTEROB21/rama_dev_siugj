                                                                                    '","arrColumas":['+arrColumnas+']}';
																	
                                                                    	var aParams=[['cadObj',bE(cadObj)]];
                                                                        enviarFormularioDatosV('../reportes/generarConciliacionBancaria.php',aParams,'POST');
                                                                        ventanaAM.close();
                                                                    
                                                                    }
														},
														{
