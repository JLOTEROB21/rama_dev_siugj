                                                                            	arrColumnas=o;
                                                                            else
                                                                            	arrColumnas+=','+o;
                                                                        }
                                                                        
                                                                        
                                                                        var cadObj='{"despachos":"'+bE(gEx('cmbDespacho').getValue())+'","periodo":"'+gEx('cmbMes').getValue()+
                                                                        			'","fechaInicio":"'+gEx('dteFechaInicio').getValue().format('Y-m-d')+
