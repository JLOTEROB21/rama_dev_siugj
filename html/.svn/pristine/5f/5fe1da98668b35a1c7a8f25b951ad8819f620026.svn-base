                                                                        {
                                                                        	msgBox('Debe selecionar almenos un campo a proyectar en el informe');
                                                                        	return;
                                                                        }
                                                                        for(x=0;x<filas.length;x++)
                                                                        {
                                                                        	o='{"columna":"'+filas[x].data.columna+'","campo":"'+filas[x].data.campo+'"}';
