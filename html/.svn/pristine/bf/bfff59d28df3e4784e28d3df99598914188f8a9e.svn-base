function irAEtapa(idFormulario,idEtapa)
{
	
	
	var arrParam=[['idFormulario',idFormulario],['edo',idEtapa],['regresarTablero','1']];
	enviarFormularioDatos('tblFormularios.php',arrParam);
}

function regresarGrid()
{
	var idFormulario=gE('idFormulario').value;
	var arrParam=[['idFormulario',idFormulario]];
	enviarFormularioDatos('tblFormularios.php',arrParam);
}