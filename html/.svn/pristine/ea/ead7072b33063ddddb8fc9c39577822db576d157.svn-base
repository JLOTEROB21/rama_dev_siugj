
function submite()
{
gE('roma').action='adminConvoca.php';
gE('roma').submit();
}

function modificar(valor1,valor2,valor3)
{
	gE('roma').action='../portal/creaConvocatoria.php';
	gE('programa').value=valor1;
	gE('programan').value=valor2;
	gE('estado').value=valor3;
	gE('roma').submit();
}

function eliminar(valor1)
{
	gE('roma').action='../portal/adminConvoca.php';
	gE('programa').value=valor1;
	gE('elimina').value='borra';
	gE('roma').submit();

}