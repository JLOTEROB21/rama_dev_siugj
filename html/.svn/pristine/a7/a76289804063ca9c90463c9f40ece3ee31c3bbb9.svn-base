function validarRadio(chk,checked)
{
	if(!checked)
    	return;
	var arrId=chk.id.split('_');
	var cmbDepartamento=gEx('cmbDepartamento');

	switch(arrId[1])
    {
    	case '1':
        case '2':
            gE('hUnidad').value=-1;
            cmbDepartamento.reset();
			cmbDepartamento.disable();        
            
        break;
        case '3':
        	cmbDepartamento.enable();
        break;
    }
}

